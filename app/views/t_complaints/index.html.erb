<h1>Compaint List</h1>

<table><tr><td ><h3>Complaint Details:</h3></td><td><%= link_to 'New Complaint', new_t_complaint_path %></td></tr></table>
<table border="1">

  <thead>
    <tr>
      <th>Complaint#</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Middle Name</th>
    <th>Complaint Details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @t_complaints.each do |t_complaint| %>
      <tr>
        <td><%= t_complaint.cmpnumber %></td>
        <td><%= t_complaint.cmpfirst %></td>
        <td><%= t_complaint.cmpmiddle %></td>
        <td><%= t_complaint.cmplast %></td>
        <td><%= t_complaint.cmpdetails %></td>
        <td><%= link_to 'Show', t_complaint %></td>
        <td><%= link_to 'Edit', edit_t_complaint_path(t_complaint) %></td>
        <td><%= link_to 'Destroy', t_complaint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


